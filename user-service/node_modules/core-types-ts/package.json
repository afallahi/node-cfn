{
  "name": "core-types-ts",
  "version": "2.0.0",
  "description": "core-types ⬌ TypeScript interface conversion",
  "author": "Gustaf Räntilä",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/grantila/core-types-ts/issues"
  },
  "homepage": "https://github.com/grantila/core-types-ts#readme",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {},
  "engines": {
    "node": ">=12"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "./node_modules/.bin/rimraf dist && ./node_modules/.bin/tsc -p tsconfig.prod.json",
    "test": "node_modules/.bin/jest --coverage",
    "cz": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/grantila/core-types-ts"
  },
  "keywords": [
    "type",
    "types",
    "generic",
    "typescript",
    "json"
  ],
  "dependencies": {
    "core-types": "^1.10.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "jest": "^28.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__snapshots__/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
